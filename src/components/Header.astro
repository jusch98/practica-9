---
const links = [
  { name: "Inicio", href: "/" },
  { name: "Cursos", href: "/courses" },
  { name: "Buscar", href: "/"},
  { name: "Contacto", href: "/contact" },
];
---

<header class="bg-white p-2 fixed top-0 left-0 w-full z-50">
  <div class="container mx-auto flex justify-between items-center">
    <img src="/Logo_nike.jpg" class="w-24 h-auto">
    
    <nav>
      <ul class="flex space-x-4">
        {links.map((link) => (
          <li>
            <a href={link.href} class="hover:underline text-black font-Akira h-10">{link.name}</a>
          </li>
        ))}
      </ul>
    </nav>
  </div>
</header>
